<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="org.yoon.mapper.AttachMapper">

	<insert id="insert">
		insert into attach (uuid, uploadPath, fileName, fileType, bno)
		values (#{uuid}, #{uploadPath}, #{fileName}, #{fileType}, #{bno})
	</insert>
	
	<delete id="delete">
		delete from attach where uuid = #{uuid}
	</delete>
	
	<select id="findByBno" resultType="org.yoon.domain.AttachVO">
		select * from attach where bno = #{bno}
	</select>

</mapper>